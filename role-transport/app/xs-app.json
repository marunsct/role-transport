{
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "authenticationType": "xsuaa",
      "destination": "srv-api",
      "csrfProtection": false
    },
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
  },
  {
    "source": "/(.*)",
    "localDir": ".",
    "authenticationType": "xsuaa"
  }
  ],
  "welcomeFile": "app/transport/index.html"
}